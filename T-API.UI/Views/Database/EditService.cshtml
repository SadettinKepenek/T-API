@model T_API.UI.Models.Database.EditServiceViewModel
@{
    ViewData["Title"] = "EditService";
}

<script src="~/js/DatabaseCrud.js"></script>
<div class="form-group">
    <label asp-for="Username">Database Username</label>
    <input type="text" class="form-control" placeholder="Enter Username" asp-for="Username" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="Username"></span>
</div>

<div class="form-group">
    <label asp-for="DatabaseName">Database Name</label>
    <input type="text" class="form-control" placeholder="" asp-for="DatabaseName" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="DatabaseName"></span>
</div>
<div class="form-group">
    <label asp-for="Server">Server</label>
    <input type="text" class="form-control" placeholder="" asp-for="Server" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="Server"></span>
</div>
<div class="form-group">
    <label asp-for="Port">Port</label>
    <input type="text" class="form-control" placeholder="" asp-for="Port" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="Port"></span>
</div>
<div class="form-group">
    <label asp-for="Provider">Provider</label>
    <input type="text" class="form-control" placeholder="" asp-for="Provider" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="Provider"></span>
</div>
<div class="form-group">
    <label asp-for="StartDate">Start Date</label>
    <input type="datetime-local" class="form-control" placeholder="" asp-for="StartDate" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="StartDate"></span>
</div>
<div class="form-group">
    <label asp-for="EndDate">End Date</label>
    <input type="datetime-local" class="form-control" placeholder="" asp-for="EndDate" disabled="disabled" readonly="readonly">
    <span class="text-danger" asp-validation-for="EndDate"></span>
</div>
<input asp-for="UserId" hidden="hidden" />
<input asp-for="DatabaseId" hidden="hidden" />


